{
    "name": "foldui-editor",
    "version": "0.0.1",
    "description": "Visual Editor for page builder",
    "type": "module",
    "main": "./dist/foldui-editor.umd.js",
    "module": "./dist/foldui-editor.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/foldui-editor.es.js",
